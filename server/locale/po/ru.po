#: src/core/auth/AuthHandler.cpp:42
#, c-format
msgid "Registration"
msgstr "Регистрация"

#: src/core/auth/AuthHandler.cpp:43
#, c-format
msgid ""
"{999999}Welcome to #RED#Oasis #WHITE#Freeroam, #DEEP_SAFFRON#%s\n"
"\n"
"\n"
"#LIGHT_GRAY#> This name is not registered\n"
"#LIGHT_GRAY#> Please register with a valid password\n"
"#LIGHT_GRAY#> Acceptable passwords are at least 6 characters long\n"
"\n"
"\n"
"#LIGHT_GRAY#> If you have any trouble, please visit our discord server or "
"contact any staff member:\n"
"#DEEP_SAFFRON#oasisfreeroam.xyz"
msgstr ""

#: src/core/auth/AuthHandler.cpp:51 src/core/auth/AuthHandler.cpp:234
msgid "Enter"
msgstr "Ввести"

#: src/core/auth/AuthHandler.cpp:52 src/core/auth/AuthHandler.cpp:95
msgid "Quit"
msgstr "Выйти"

#: src/core/auth/AuthHandler.cpp:81 src/core/auth/AuthHandler.cpp:94
#, c-format
msgid "Login"
msgstr "Войти"

#: src/core/auth/AuthHandler.cpp:82
#, c-format
msgid ""
"#RED#Wrong Password #LIGHT_GRAY#entered for %s #RED#[%d/3]\n"
"#LIGHT_GRAY#Please re-write the correct password in the field below to "
"login.\n"
"\n"
"#LIGHT_GRAY#- If you have forgotten your password, request a password "
"recovery at our discord server:\n"
"#DEEP_SAFFRON#oasisfreeroam.xyz"
msgstr ""

#: src/core/auth/AuthHandler.cpp:88
#, c-format
msgid ""
"#LIGHT_GRAY#Welcome back to #RED#Oasis #WHITE#Freeroam #DEEP_SAFFRON#%s\n"
"\n"
"#LIGHT_GRAY#- Your name is registered in our database\n"
"#LIGHT_GRAY#- Login by entering your password\n"
"\n"
"#LIGHT_GRAY#- If you have forgotten your password, request a password "
"recovery at our discord server:\n"
"#DEEP_SAFFRON#oasisfreeroam.xyz"
msgstr ""
"#LIGHT_GRAY#Добро пожаловать снова на #RED#Oasis#WHITE# Freeroam, "
"#DEEP_SAFFRON#%s\n"
"\n"
"#LIGHT_GRAY#- Ваше имя зарегистрировано в нашей базе данных\n"
"#LIGHT_GRAY#- Войдите с помощью своего пароля\n"
"\n"
"#LIGHT_GRAY#- Если вы забыли пароль, запросите смену пароля на нашем Discord-"
"сервере:\n"
"#DEEP_SAFFRON#oasisfreeroam.xyz"

#: src/core/auth/AuthHandler.cpp:122
msgid "You have been logged in!"
msgstr "Вы вошли в аккаунт!"

#: src/core/auth/AuthHandler.cpp:136
msgid "Too much login attempts!"
msgstr "Слишком много попыток логина!"

#: src/core/auth/AuthHandler.cpp:147
msgid "Password length must be between 6-48"
msgstr "Пароль должен быть длиной от 6 до 48 символов"

#: src/core/auth/AuthHandler.cpp:181
msgid "Something went wrong when trying to create user!"
msgstr "Что-то пошло не так, пока мы создавали аккаунт!"

#: src/core/auth/AuthHandler.cpp:227
#, c-format
msgid "Enter your email"
msgstr "Введите ваш email"

#: src/core/auth/AuthHandler.cpp:228
msgid ""
"#LIGHT_GRAY#Please enter your #WHITE#Email address#LIGHT_GRAY#.\n"
"\n"
"Use a correct email address as it can be used for password recovery\n"
"to restore your account incase you forget your password\n"
"You can skip entering email, but account features will be limited! Use "
"#WHITE#/verify#LIGHT_GRAY# later"
msgstr ""

#: src/core/auth/AuthHandler.cpp:235
msgid "Skip"
msgstr "Пропустить"

#: src/core/auth/AuthHandler.cpp:260
msgid "Invalid email!"
msgstr "Неверный email!"

#: src/core/auth/AuthHandler.cpp:307
#, c-format
msgid "Registration info"
msgstr "Информация о регистрации"

#: src/core/auth/AuthHandler.cpp:308
#, c-format
msgid ""
"#DRY_HIGHLIGHTER_GREEN#Your account has been successfully registered and "
"logged in.\n"
"\n"
"#MILLION_GREY#Username: #WHITE#%s\n"
"#MILLION_GREY#Account ID: #WHITE#%d\n"
"#MILLION_GREY#Password: #WHITE#%s\n"
"#MILLION_GREY#Registration date: #WHITE#%s\n"
"\n"
"#MILLION_GREY#Use '#RED#F8#MILLION_GREY#' to take screenshot and save the "
"password."
msgstr ""

#: src/core/auth/AuthHandler.cpp:319
msgid "OK"
msgstr "ОК"

#: src/core/auth/AuthHandler.cpp:322
msgid "You have successfully registered!"
msgstr "Вы успешно зарегистрировались"

